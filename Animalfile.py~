class Animal(object):
	def __init__(self,sound,name,age):
		self.sound=sound
		self.name=name
		self.age=age
	def dreaming(self, World):
		print("wow")
	def eat(self, chips):
		print("yumy")
	def play(self, ball):
		print("goal")
	def sting(self, person):
		print("ouch")
	def hit(self, wall):
		print("ow")
	def kill(self, lions):
		print("ahgg")
	def step(self, things):
		print("psht")
	def move(self, wings):
		print("ofof")
	def walk(self, floor):
		print("tttt")
	def cry(self, food):
		print("goood")
